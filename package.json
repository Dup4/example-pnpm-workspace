{
  "name": "@dup4/example-pnpm-workspace",
  "version": "0.0.1",
  "packageManager": "pnpm@7.1.7",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/Dup4/example-pnpm-workspace#readme",
  "bugs": {
    "url": "https://github.com/Dup4/example-pnpm-workspace/issues"
  },
  "license": "MIT",
  "author": "Dup4 <lyuzhi.pan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dup4/example-pnpm-workspace.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "src",
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "start": "esmo src/index.ts",
    "test": "vitest",
    "lint": "run-p lint:*",
    "lint:build": "tsc --noEmit",
    "lint:eslint": "eslint .",
    "lint:style": "prettier --check src/*.ts src/**/*.ts",
    "format": "run-p format:*",
    "format:eslint": "eslint --fix .",
    "format:style": "prettier --write src/*.ts src/**/*.ts",
    "prepublishOnly": "npm run build",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "devDependencies": {
    "@babel/types": "^7.18.0",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "bumpp": "^7.1.1",
    "eslint": "^8.16.0",
    "esmo": "^0.14.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.1.7",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.7.13"
  }
}
